<p>I'm in Questionnaire show</p>
=simple_form_for @institute, :method => :patch, html: { class: 'form-horizontal' } do |f|
  -@questions.each do |question|
    %p= question.content
    .inputs
      = f.input :id, collection: @lecturers, :label_method => lambda { |lect| "#{lect.lastname} #{lect.firstname}" }
  .action
    = f.button :submit, 'Dodaj wykładowcę', class: 'btn btn-primary'
